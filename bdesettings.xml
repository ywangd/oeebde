<?xml version="1.0" encoding="UTF-8"?>
<BdeSettings>
	<Validations>
		<Action>
			<Rule Name="RCode"></Rule>
			<Rule Name="ECode"></Rule>
			<Rule Name="ACode"></Rule>
			<Rule Name="FACodeZeroJobID"></Rule>
			<Rule Name="FACodeNotMRor@95"></Rule>
			<Rule Name="ECodeChanges"></Rule>
			<Rule Name="TSLess"></Rule>
			<Rule Name="TSFuture"></Rule>
			<Rule Name="ITLess"></Rule>
			<Rule Name="ITOutbound"></Rule>
			<Rule Name="No@95"></Rule>
		</Action>
		<Rules>
			<Rule Name="RCode" Module="bdevalidationrules" Function="rule_1">
				<Code>1</Code>
				<Description>Record code not in master table</Description>
			</Rule>
			<Rule Name="ECode" Module="bdevalidationrules" Function="rule_2">
				<Code>2</Code>
				<Description>Equipment code not in master table</Description>
			</Rule>
			<Rule Name="ACode" Module="bdevalidationrules" Function="rule_3">
				<Code>3</Code>
				<Description>Activity code not in master table</Description>
			</Rule>
			<Rule Name="FACodeZeroJobID" Module="bdevalidationrules" Function="rule_5">
				<Code>5</Code>
				<Description>First significant Activity code has a zero Job ID
				</Description>
			</Rule>
			<Rule Name="FACodeNotMRor@95" Module="bdevalidationrules"
				Function="rule_6">
				<Code>6</Code>
				<Description>First significant Activity code not MR or @95
				</Description>
			</Rule>
			<Rule Name="ECodeChanges" Module="bdevalidationrules" Function="rule_8">
				<Code>8</Code>
				<Description>Equipment code not consistent</Description>
			</Rule>
			<Rule Name="TSLess" Module="bdevalidationrules" Function="rule_9">
				<Code>9</Code>
				<Description>Timestamp is less than the prior record</Description>
			</Rule>
			<Rule Name="TSFuture" Module="bdevalidationrules" Function="rule_10">
				<Code>10</Code>
				<Description>Timestamp is in the future</Description>
			</Rule>
			<Rule Name="ITLess" Module="bdevalidationrules" Function="rule_12">
				<Code>12</Code>
				<Description>Impression Total is less than the prior record
				</Description>
			</Rule>
			<Rule Name="ITOutbound" Module="bdevalidationrules" Function="rule_14">
				<Code>14</Code>
				<Description>Impression Total not in range of 1-999 million
				</Description>
			</Rule>
			<Rule Name="No@95" Module="bdevalidationrules" Function="rule_15">
				<Code>15</Code>
				<Description>File contains no @95</Description>
			</Rule>
		</Rules>
	</Validations>
	<Sumups>
		<Categories>
			<Category Name="MR">
				<Member Name="MR"></Member>
				<Member Name="@95"></Member>
				<StartRule Name="Whenever"></StartRule>
				<TerminateRule Name="All"></TerminateRule>
				<EndRule Name="ByOthers"></EndRule>
			</Category>
			<Category Name="Prod">
				<Member Name="Prod">
				</Member>
				<StartRule Name="Whenever">
				</StartRule>
				<TerminateRule Name="All"></TerminateRule>
				<EndRule Name="ByOthers"></EndRule>
			</Category>
			<Category Name="W-up">
				<Member Name="W-up">
					<Status>
						<Contains>ON</Contains>
						<Contains>OFF</Contains>
						<Except>Nothing</Except>
					</Status>
					<Code>
						<Except>999</Except>
					</Code>
				</Member>
				<StartRule Name="W-up"></StartRule>
				<TerminateRule Name="None"></TerminateRule>
				<EndRule Name="W-up"></EndRule>
			</Category>
			<Category Name="Downtime">
				<Member Name="Downtime">
				</Member>
				<StartRule Name="Whenever"></StartRule>
				<TerminateRule Name="None"></TerminateRule>
				<EndRule Name="ByOthers"></EndRule>
			</Category>
			<Category Name="JobEnd">
				<Member Name="@97"></Member>
				<StartRule Name="Whenever"></StartRule>
				<TerminateRule Name="All"></TerminateRule>
				<EndRule Name="ByOthers"></EndRule>
			</Category>
		</Categories>
		<Rules>
			<StartRules>
				<Whenever Module="bdesumuprules" Function="rule_SR_Whenever"></Whenever>
				<OnCode Module="bdesumuprules" Function="rule_SR_OnCode"></OnCode>
				<W-up Module="bdesumuprules" Function="rule_SR_Wup"></W-up>
			</StartRules>
			<TerminateRules>
				<All Module="bdesumuprules" Function="rule_TR_All"></All>
				<None Module="bdesumuprules" Function="rule_TR_None"></None>
			</TerminateRules>
			<EndRules>
				<ByOthers Module="bdesumuprules" Function="rule_ER_ByOthers"></ByOthers>
				<OnCode Module="bdesumuprules" Function="rule_ER_OnCode"></OnCode>
				<W-up Module="bdesumuprules" Function="rule_ER_Wup"></W-up>
			</EndRules>
		</Rules>
	</Sumups>
	<Reporting>
		<Categories>
			<Category Name="MR">
				<Rules>
					<Rule Name="Concatenate">
						<Variable Name="SIG_DURATION" Type="float" List="0"
							Value="0.08333333">
						</Variable>
						<Variable Name="SIG_IMPCOUNT" Type="int" List="0" Value="20"></Variable>
					</Rule>
				</Rules>
			</Category>
			<Category Name="Prod">
				<Rules>
					<Rule Name="ConvertIfCode">
						<Variable Name="from_sumup" Type="str" List="0" Value="Prod"></Variable>
						<Variable Name="to_sumup" Type="str" List="0" Value="MR"></Variable>
						<Variable Name="onlycodes" Type="str" List="1" Value="@118"></Variable>
						<Variable Name="onlycodes" Type="str" List="1" Value="@119"></Variable>
					</Rule>
					<Rule Name="Concatenate">
						<Variable Name="SIG_DURATION" Type="float" List="0"
							Value="0.08333333">
						</Variable>
						<Variable Name="SIG_IMPCOUNT" Type="int" List="0" Value="20"></Variable>
					</Rule>
				</Rules>
			</Category>
			<Category Name="W-up">
				<Rules>
					<Rule Name="None"></Rule>
				</Rules>
			</Category>
			<Category Name="Downtime">
				<Rules>
					<Rule Name="None"></Rule>
				</Rules>
			</Category>
		</Categories>
		<Rules>
			<Rule Name="None" Module="reportingrules" Function="rule_None"></Rule>
			<Rule Name="Concatenate" Module="reportingrules" Function="rule_Concatenate"></Rule>
			<Rule Name="ConvertIfCode" Module="reportingrules" Function="rule_ConvertIfCode"></Rule>
		</Rules>
	</Reporting>
</BdeSettings>

