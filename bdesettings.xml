<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="bdesettings.xsl" ?>
<BdeSettings xsi:noNamespaceSchemaLocation="bdesettings.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<Header>
		<Client>
			<Name>Unknown Client</Name>
		</Client>
		<Operator>
			<Name>Anonymous</Name>
			<Date>2001-12-31T12:00:00</Date>
		</Operator>
	</Header>
	<General>
		<InputFile>good.bde</InputFile>
		<OutputFile></OutputFile>
		<Verbose>true</Verbose>
		<Debug>true</Debug>
	</General>
	<Validations>
		<Action>
			<Rule Name="RCode"></Rule>
			<Rule Name="ECode"></Rule>
			<Rule Name="ACode"></Rule>
			<Rule Name="FACodeZeroJobID"></Rule>
			<Rule Name="FACodeNotMRor@95"></Rule>
			<Rule Name="ECodeChanges"></Rule>
			<Rule Name="TSLess"></Rule>
			<Rule Name="TSFuture"></Rule>
			<Rule Name="ITLess"></Rule>
			<Rule Name="ITOutbound"></Rule>
			<Rule Name="No@95"></Rule>
		</Action>
	</Validations>
	<Sumups>
		<Categories>
			<Category Name="MR">
				<Member Name="MR"></Member>
				<Member Name="@95"></Member>
				<StartRule Name="Whenever"></StartRule>
				<TerminateRule Name="All"></TerminateRule>
				<EndRule Name="ByOthers"></EndRule>
			</Category>
			<Category Name="Prod">
				<Member Name="Prod">
				</Member>
				<StartRule Name="Whenever">
				</StartRule>
				<TerminateRule Name="All"></TerminateRule>
				<EndRule Name="ByOthers"></EndRule>
			</Category>
			<Category Name="W-up">
				<Member Name="W-up">
					<Status>
						<Contains>ON</Contains>
						<Contains>OFF</Contains>
						<Except>Nothing</Except>
					</Status>
					<Code>
						<Except>999</Except>
					</Code>
				</Member>
				<StartRule Name="W-up"></StartRule>
				<TerminateRule Name="None"></TerminateRule>
				<EndRule Name="W-up"></EndRule>
			</Category>
			<Category Name="Downtime">
				<Member Name="Downtime">
				</Member>
				<StartRule Name="Whenever"></StartRule>
				<TerminateRule Name="None"></TerminateRule>
				<EndRule Name="ByOthers"></EndRule>
			</Category>
			<Category Name="JobEnd">
				<Member Name="@97"></Member>
				<StartRule Name="Whenever"></StartRule>
				<TerminateRule Name="All"></TerminateRule>
				<EndRule Name="ByOthers"></EndRule>
			</Category>
		</Categories>
	</Sumups>
	<Reporting>
		<Categories>
			<Category Name="MR">
				<Rules>
					<Rule Name="Concatenate">
						<Variable Name="SIG_DURATION" Type="float" List="false"
							Value="0.08333333">
						</Variable>
						<Variable Name="SIG_IMPCOUNT" Type="int" List="false" Value="20"></Variable>
					</Rule>
				</Rules>
			</Category>
			<Category Name="Prod">
				<Rules>
					<Rule Name="ConvertIfCode">
						<Variable Name="from_sumup" Type="str" List="false" Value="Prod"></Variable>
						<Variable Name="to_sumup" Type="str" List="false" Value="MR"></Variable>
						<Variable Name="onlycodes" Type="str" List="true" Value="@118"></Variable>
						<Variable Name="onlycodes" Type="str" List="true" Value="@119"></Variable>
					</Rule>
					<Rule Name="Concatenate">
						<Variable Name="SIG_DURATION" Type="float" List="false"
							Value="0.08333333">
						</Variable>
						<Variable Name="SIG_IMPCOUNT" Type="int" List="false" Value="20"></Variable>
					</Rule>
				</Rules>
			</Category>
			<Category Name="W-up">
				<Rules>
					<Rule Name="None"></Rule>
				</Rules>
			</Category>
			<Category Name="Downtime">
				<Rules>
					<Rule Name="None"></Rule>
				</Rules>
			</Category>
		</Categories>
	</Reporting>
</BdeSettings>

